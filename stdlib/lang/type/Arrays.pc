import stdlib.lang.Casting

#Returns the amount of elements in an array.
func len(text[] v) -> int: return v;

#Swap two values in an array.
func swap(var[] a, int i, int j) -> var[] {
	nr temp = a[i]
	a[i] = a[j]
	a[j] = temp
	return a
}

#Reverses the order of all elements.
func reverse(var[] a) -> var[] {
	var[] res = a
	for e in a: res[a - 1 - i] = e;
	return res
}

#Creates a new Array with the specified length.
func newArray(int len) -> var[] {
	return [0] * len
}