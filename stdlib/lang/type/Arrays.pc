import stdlib.lang.Casting

#Returns the amount of elements in an array.
func length(var[] a) -> nr: return a;

#Swap two values in an array.
func swap(var[] a, nr i, nr j) -> var[] {
	nr temp = a[int(i)]
	a[int(i)] = a[int(j)]
	a[int(j)] = temp
	return a
}

func reverse(var[] a) -> var[] {
	var[] res = [0] * int(a)
	for e in a: res[int(a) - 1 - i] = e;
	return res
}

func newArray(nr length) -> var[] {
	return [0] * length
}